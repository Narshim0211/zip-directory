import React from "react";import { BrowserRouter as Router, Routes, Route } from "react-router-dom";import HomePage from "./components/HomePage";import AdminDashboard from "./components/AdminDashboard";import BusinessDetails from "./components/BusinessDetails";import VisitorPage from "./components/VisitorPage";import OwnerPage from "./components/OwnerPage";// Inside <Routes>:function App() {  return (    <Router>      <Routes>        {/* Homepage */}        <Route path="/" element={<HomePage />} />        {/* Admin Dashboard */}        <Route path="/admin" element={<AdminDashboard />} />        {/* Placeholder routes */}        <Route          path="/owner"          element={<h2 style={{ textAlign: "center", marginTop: 50 }}>Owner Dashboard (Coming Soon)</h2>}        />        <Route          path="/visitor"          element={<h2 style={{ textAlign: "center", marginTop: 50 }}>Explore Salons (Coming Soon)</h2>}        />        {/* Business Details */}        <Route path="/business/:id" element={<BusinessDetails />} />        {/* Visitor Page */}        <Route path="/visitor" element={<VisitorPage />} />        {/* Owner Page */}        <Route path="/owner" element={<OwnerPage />} />      </Routes>    </Router>  );}export default App;